package aplicacao;

import dominio.Pessoa;

public class Programa {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa(1, "Dennys", "dennys.schauer@hotmail.com");
		Pessoa p2 = new Pessoa(2, "Solange", "solange.schauer@hotmail.com");
		Pessoa p3 = new Pessoa(3, "Miguel", "miguel.santos@hotmail.com");
		
		System.out.println(p1);
		System.out.println(p2);
		System.out.println(p3);

	}

}
